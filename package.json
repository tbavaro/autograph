{
  "name": "graphit",
  "homepage": ".",
  "version": "0.2.1",
  "private": true,
  "dependencies": {
    "@material/top-app-bar": "^0.32.0",
    "classnames": "^2.2.5",
    "d3": "^4.12.0",
    "gapi": "0.0.3",
    "material-components-web": "^0.32.0",
    "material-icons": "^0.1.0",
    "node-sass-chokidar": "^1.0.1",
    "npm": "^5.7.1",
    "npm-run-all": "^4.1.2",
    "query-string": "^5.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts-ts": "2.13.0",
    "request-promise-native": "^1.0.5",
    "sanitize-html": "^1.18.2",
    "tsvalidators": "github:tbavaro/tsvalidators#dist"
  },
  "scripts": {
    "start-ts": "react-scripts-ts start",
    "build-ts": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-css": "node-sass-chokidar --importer sass-custom-importer.js --include-path ./src --include-path ./node_modules src/ -o src/ -",
    "watch-css": "npm run build-css && node-sass-chokidar --importer sass-custom-importer.js --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-ts",
    "build": "npm-run-all build-css build-ts",
    "push": "./node_modules/.bin/tsdevtools deploy --branch gh-pages --subtreeDir build --moveNodeModulesTemporarily"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/d3": "^4.12.0",
    "@types/gapi": "0.0.35",
    "@types/gapi.auth2": "0.0.47",
    "@types/gapi.client.drive": "^3.0.0",
    "@types/gapi.client.sheets": "^4.0.0",
    "@types/google.picker": "0.0.31",
    "@types/jest": "^22.1.3",
    "@types/material-components-web": "^0.26.0",
    "@types/node": "^9.4.6",
    "@types/query-string": "^5.1.0",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/request-promise-native": "^1.0.14",
    "@types/sanitize-html": "^1.14.0",
    "autogents": "github:tbavaro/autogents#dist",
    "tsdevtools": "github:tbavaro/tsdevtools#dist",
    "typescript": "^2.8.1"
  }
}
