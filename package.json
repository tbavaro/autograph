{
  "name": "autograph",
  "homepage": ".",
  "version": "0.15.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "classnames": "^2.2.6",
    "d3": "^4.13.0",
    "fuse.js": "^3.4.4",
    "query-string": "^6.4.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tsvalidators": "github:tbavaro/tsvalidators#dist"
  },
  "scripts": {
    "build:css": "node-sass src/ -o src/ ",
    "watch:css": "npm run build:css && node-sass src/ -o src/ -w -r",
    "start:js": "react-scripts-ts start",
    "start": "npm-run-all -p watch:css start:js",
    "build:js": "react-scripts-ts build",
    "build": "npm-run-all build:*",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "codegen": "./node_modules/.bin/generate-validators --output src/generated/GraphDataValidators.ts src/data/GraphData.ts",
    "push": "./node_modules/.bin/tsdevtools deploy --branch gh-pages --subtreeDir build",
    "pushbeta": "./node_modules/.bin/tsdevtools deploy --branch gh-pages-beta --subtreeDir build --version-bump beta && git fetch origin gh-pages gh-pages-beta && git checkout gh-pages-beta && git checkout gh-pages && rm -rf beta && git clone .git beta --branch gh-pages-beta --single-branch && rm -rf beta/.git && git add -A beta && git commit -m \"beta\" && git push && git checkout master && git branch -D gh-pages gh-pages-beta"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/d3": "^4.13.2",
    "@types/gapi": "0.0.35",
    "@types/gapi.auth2": "0.0.47",
    "@types/gapi.client.drive": "^3.0.3",
    "@types/gapi.client.sheets": "^4.0.1",
    "@types/google.picker": "0.0.31",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.0",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "autogents": "github:tbavaro/autogents#dist",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "react-scripts-ts": "3.1.0",
    "source-map-explorer": "^1.8.0",
    "tsdevtools": "github:tbavaro/tsdevtools#dist",
    "typescript": "^3.4.2"
  }
}
